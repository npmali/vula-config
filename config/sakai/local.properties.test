# All configuration settings which involve server domain name
# Localized here so that test servers can use devslscle001.uct.ac.za

serverName = devslscle001.uct.ac.za
serverUrl = https://devslscle001.uct.ac.za

# Outgoing email default address (for course requests)
setup.request=help@devslscle001.uct.ac.za

# Email address quoted in bounce messages to incoming mail
support.email=help@devslscle001.uct.ac.za

# Used in some other contexts
mail.support=help@devslscle001.uct.ac.za
mail.support.name=Vula Help

# Tsugi and Opencast dev launch URLs
sakai.opencast.setup.launch=https://tsugidev.uct.ac.za/mod/tsugi-welcome-to-lecture-recording/
sakai.opencast.series.launch=https://mediadev.uct.ac.za/lti

# Default sender (SMTP FROM) address
smtpFrom@org.sakaiproject.email.api.EmailService=bounces@devslscle001.uct.ac.za

# email address to send errors caught by the portal
portal.error.email=help@devslscle001.uct.ac.za

log.config.count = 2
log.config.1=DEBUG.org.sakaiproject.login
log.config.2=DEBUG.org.springframework.security
#log.config.3=DEBUG.org.sakaiproject.db.impl.BasicSqlService

# enable hibernate SQL debugging output
#hibernate.show_sql=true

testMode@org.sakaiproject.email.api.EmailService=true

profilewow.useSearch=true
search.experimental.didyoumean=true

#test skin
# skin.default=greenweek
elasticsearch.discovery.zen.ping.unicast.hosts=devslscle001.uct.ac.za:9300,devslscle001.uct.ac.za:9301
elasticsearch.node.name=vuladev-1a
elasticsearch.node.data=false

# Turnitin API - specified here so test servers can disable this
turnitin.apiURL=https://api.turnitin.com/api.asp?

# Log errors for "Your HTML contains ..."
content.cleaner.errors.logged=true

#memory.SecurityService.debug=true

